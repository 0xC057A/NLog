<?xml version="1.0" ?>
<nlog xml:space="preserve">
    <extensions>
        <add assembly="MyAssembly" />
    </extensions>

    <appenders>
        <appender name="console" type="Console" layout="${hour:showminutes=false} ${message}" />
    </appenders>

    <rules>
        <logger name="*" minLevel="Info" appendTo="console" />
    </rules>
</nlog>
