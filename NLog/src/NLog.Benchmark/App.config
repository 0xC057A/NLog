<?xml version="1.0" ?>
<configuration>
    <configSections>
        <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    </configSections>
    <nlog autoReload="true">
        <appenders>
            <appender name="null" type="Null" />
        </appenders>

        <rules>
            <logger name="*" minlevel="Info" appendTo="null" />
        </rules>
    </nlog>
</configuration>
