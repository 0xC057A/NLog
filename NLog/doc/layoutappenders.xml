    </la>
    <la name="machinename">
        <displayName>Machine Name</displayName>
        <description>outputs the machine name that the application is running on</description>
        <assembly>NLog.dll</assembly>
        <namespace>NLog.LayoutAppenders</namespace>
        <className>MachineNameLayoutAppender</className>
        <support framework="net-1.0" />
